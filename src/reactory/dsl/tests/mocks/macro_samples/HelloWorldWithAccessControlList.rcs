
acl.required([ADMIN, USER]) {
  const message = "Hello";
  @ask("What is your name?")-->@var("name", $out)-->@print(`$message $out`);
  const hobbies = @ask("What are your hobbies?");
  @llm(`Suggest some places $name could visit based on their $hobbies`)
} else {
  @ask("Hi, you don't have access to this module")
  exit "Permission denied"
}
