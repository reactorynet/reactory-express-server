{
  "name": "@reactory/server-core",
  "version": "0.7.9",
  "displayName": "Reactory Server",
  "description": "Reactory Core Server",
  "main": "server.js",
  "module": "reactory-api",
  "author": {
    "name": "Werner Weber",
    "email": "werner.weber@reactory.net"
  },
  "licenses": [
    {
      "license": "Apache-2.0"
    }
  ],
  "scripts": {
    "start": "NODE_PATH=./src npx nodemon --exec npx babel-node ./src/server.js --presets @babel/env --max_old_space_size=2000000",
    "start:prod": "NODE_PATH=./src env-cmd -f ./config/.env.production npx nodemon --exec npx babel-node ./src/server.js --presets @babel/env --max_old_space_size=2000000",
    "start:reactory": "NODE_PATH=./src env-cmd -f ./config/.env.reactory.local npx nodemon --exec npx babel-node ./src/server.js --presets @babel/env --max_old_space_size=2000000",
    "start:reactory:drew": "NODE_PATH=./src env-cmd -f ./config/.env.local.drew npx nodemon --exec npx babel-node ./src/server.js --presets @babel/env --max_old_space_size=2000000",
    "start:lasec-crm": "NODE_PATH=./src env-cmd -f ./config/.env.lasec.local npx nodemon --exec npx babel-node ./src/server.js --presets @babel/env --max_old_space_size=2000000",
    "build": "npx babel ./src --out-dir dist --presets @babel/env --copy-files",
    "set:env:production": "env-cmd ./.env.production",
    "build:prod": "npm-run-all set:env:production build",
    "serve-b": "NODE_PATH=. babel-node ./server.js --preset=es2016,stage-2",
    "serve": "pm2 start config/run/$BUILD_TARGET.config.js",
    "debug:prod": "NODE_PATH=./src env-cmd -f ./config/.env.production npx nodemon --exec npx babel-node ./src/server.js --inspect --presets @babel/env --max_old_space_size=2000000",
    "debug:reactory": "NODE_PATH=./src env-cmd -f ./config/.env.reactory.local npx nodemon --exec npx babel-node ./src/server.js --inspect --presets @babel/env --max_old_space_size=2000000",
    "debug:lasec-crm": "NODE_PATH=./src env-cmd -f ./config/.env.lasec.local npx nodemon --exec npx babel-node ./src/server.js --inspect --presets @babel/env --max_old_space_size=2000000",
    "debug": "NODE_PATH=./src npx nodemon --exec npx babel-node ./src/server.js --inspect --presets @babel/env --max_old_space_size=2000000",
    "clean": "rimraf ./dist/",
    "rollup": "rollup --config rollup.config.dev.js",
    "build-serve": "npm-run-all clean build serve",
    "make-install": "npm install & npm run build-serve",
    "deploy": "pm2 deploy ./config/run/$BUILD_TARGET.config.js production deploy",
    "setup-production": "pm2 deploy ./config/run/$BUILD_TARGET.config.js production setup",
    "test": "mocha",
    "reactory:create-plugin": "NODE_PATH=./src npx babel-node ./src/scripts/create-plugin.js --presets @babel/env --max_old_space_size=200000",
    "reactory:create-form": "NODE_PATH=./src npx babel-node ./src/scripts/create-form.js --presets @babel/env --max_old_space_size=200000",
    "lint": "eslint . --fix"
  },
  "dependencies": {
