{
  "name": "reactory-api",
  "version": "1.0.0",
  "description": "graphql api server for TowerStone and PLC Assesor Toolsets.",
  "main": "server.js",
  "module": "reactory-api",
  "author": "Werner Weber",
  "license": "MIT",
  "scripts": {
    "start-win": "set NODE_PATH=. && nodemon ./server.js --exec babel-node --preset=es2016,stage-2",
    "start": "NODE_PATH=./src npx babel-node ./src/server.js --presets @babel/env",
    "start-prod": "NODE_PATH=. APP_DATA_ROOT=/data/reactory LEGACY_APP_DATA_ROOT=/data/legacy API_URI_ROOT=https://api.reactory.net/  CDN_ROOT=https://api.reactory.net/cdn/ nodemon ./server.js --exec babel-node --preset=es2016,stage-2",
    "build": "npx babel src --out-dir dist --presets @babel/env --copy-files",
    "serve-b": "NODE_PATH=. babel-node ./server.js --preset=es2016,stage-2",
    "serve": "pm2 start config/run/$BUILD_TARGET.config.js",
    "start_debug": "babel-node ./server.js --inspect --preset=es2016,stage-2",
    "clean": "rimraf ./dist/",
    "rollup": "rollup --config rollup.config.dev.js",
    "build-serve": "npm-run-all clean build serve",
    "make-install": "npm install & npm run build-serve",
    "deploy": "pm2 deploy ./config/run/$BUILD_TARGET.config.js production deploy",
    "setup-production": "pm2 deploy ./config/run/$BUILD_TARGET.config.js production setup",
    "test": "mocha",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@azure/storage-blob": "^10.3.0",
    "@azure/storage-file": "^10.1.0",
    "@microsoft/microsoft-graph-client": "^1.7.0",
    "@sendgrid/client": "^6.4.0",
    "@sendgrid/mail": "^6.4.0",
    "apiai": "^4.0.3",
    "apollo-server-express": "^1.3.4",
    "body-parser": "^1.19.0",
    "botbuilder": "^3.14.1",
    "busboy": "^0.2.14",
    "chai": "^4.1.2",
    "co": "^4.6.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "email-templates": "5.0.2",
    "express": "^4.17.1",
    "express-graphql": "^0.8.0",
    "express-jwt": "^5.3.1",
    "express-jwt-permissions": "^1.0.0",
    "express-session": "^1.15.6",
    "graphql": "^14.3.1",
    "graphql-server-express": "^1.4.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^4.0.5",
    "image-type": "4.1.0",
    "javascript-state-machine": "^3.0.1",
    "jsonwebtoken": "^8.1.0",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongodb": "^3.2.7",
    "mongoose": "^5.6.0",
    "morgan": "^1.9.1",
    "mysql": "^2.14.1",
    "node-fetch": "^2.6.0",
    "object-mapper": "^5.0.0",
    "passport": "^0.4.0",
    "passport-azure-ad": "^4.0.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.10.0",
    "pdfmake": "^0.1.57",
    "pm2": "3.5.1",
    "png-js": "0.1.1",
    "png-to-jpeg": "1.0.1",
    "pngjs": "3.4.0",
    "pug": "^2.0.3",
    "restify": "^8.3.3",
    "schema-inspector": "^1.6.8",
    "sha1": "^1.1.1",
    "simple-oauth2": "^2.2.1",
    "uuid": "^3.1.0",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.3.3",
    "workflow-es": "^2.3.5",
    "workflow-es-mongodb": "^2.1.0",
    "ws": "^7.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/node": "7.4.5",
    "@babel/eslint-plugin-development": "1.0.1",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-do-expressions": "7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-bind": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.4.4",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "7.3.2",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-class-properties": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-classes": "7.4.4",
    "@babel/plugin-transform-modules-commonjs": "7.4.3",
    "@babel/plugin-transform-object-set-prototype-of-to-assign": "7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "@babel/plugin-transform-react-jsx-compat": "^7.2.0",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/plugin-transform-typescript": "7.4.0",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.4.3",
    "acorn": "6.1.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.6",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-named-asset-import": "0.3.2",
    "babel-preset-react-app": "9.0.0",
    "babel-runtime": "^6.26.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.19.1",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2",
    "rollup": "^1.16.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.4"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0,
      "no-underscore-dangle": 0,
      "linebreak-style": 1
    },
    "env": {
      "mocha": true
    }
  }
}
